<script>
    window.fbAsyncInit = function() {
        FB.init({
          appId      : '1384645955090433',
          xfbml      : true,
          version    : 'v2.0'
        });
		FB.getLoginStatus(function(response) {
		  if (response.status === 'not_authorized') {
			console.log(response.authResponse.accessToken);
			addPermistion();
		  }
		});
	};
		function addPermistion() {
		FB.login(function(response) {
			   // handle the response
			 }, {scope: 'public_profile,publish_actions,user_friends,email',auth_type: 'rerequest'});
		}
      (function(d, s, id){
         var js, fjs = d.getElementsByTagName(s)[0];
         if (d.getElementById(id)) {return;}
         js = d.createElement(s); js.id = id;
         js.src = "//connect.facebook.net/vi_VN/all.js";
         fjs.parentNode.insertBefore(js, fjs);
       }(document, 'script', 'facebook-jssdk'));



</script>

<script>
	/*$(function() {
	FB.Canvas.scrollTo(0, 0);
	});*/
	$(document).ready(function(){
		$("a").click(function(){
		scrollTo(0);
		});
	});
	function scrollTo(y){
    FB.Canvas.getPageInfo(function(pageInfo){
            $({y: pageInfo.scrollTop}).animate(
                {y: y},
                {duration: 200, step: function(offset){
                    FB.Canvas.scrollTo(0, offset);
                }
            });
		});
	}

	function sendRequestToRecipients() {
		var user_ids = document.getElementsByName("user_ids")[0].value;
		FB.ui({method: 'apprequests',
		  message: 'đã mời bạn xem ứng dụng Lịch phim',
		  to: user_ids
		}, requestCallback);
	}

	function sendRequestViaMultiFriendSelector() {
	FB.ui({method: 'apprequests',
			title: 'vCinema',
		  message: 'đã mời bạn xem ứng dụng Lịch phim',
		}, requestCallback);
	}

	function requestCallback(response) {
		// Handle callback here
	}

	function FacebookInviteFriends () {
		  FB.ui ({
		   method: 'apprequests',
		   message: 'Invite your friends'
		  });
	}



</script>
<!-- end script facebook api -->

